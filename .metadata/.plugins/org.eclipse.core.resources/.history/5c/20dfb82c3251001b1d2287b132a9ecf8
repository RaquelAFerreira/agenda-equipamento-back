package br.com.alterdata.agendaEquipamento.exceptions;

public class SolicitacaoDuplicadaException extends Exception {
	
private static final long serialVersionUID = -6840138218861403331L;
	
	private String msg = "A solicitacao informada jรก existe";
	private String codigoSolicitacao;
	
	public SolicitacaoDuplicadaException() {
		super();
	}
	public SolicitacaoDuplicadaException(String codigoSolicitacao) {
		super();
		this.codigoSolicitacao = codigoSolicitacao;
	}
	public String getMsg() {
		return msg;
	}
	public String getCodigoSolicitacao() {
		return codigoSolicitacao;
	}

}
